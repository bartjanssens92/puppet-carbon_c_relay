<%- | String $cluster_name,
      String $cluster_algorithm,
      Hash   $caches | -%>

# Fragment generated by <%= $cluster_name %>
cluster <%= $cluster_name %>
    <%= $cluster_algorithm %>
        <%- $caches.each | $cache, $settings | { -%>
        127.0.0.1:<%= $settings['line_receiver_port'] %>=<%= $cache %>
        <%- } -%>
    ;

